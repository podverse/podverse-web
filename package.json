    "awesome-debounce-promise": "2.1.0",
    "axios": "0.21.2",
    "babel-eslint": "10.x",
    "babel-loader": "8.0.6",
    "babel-plugin-root-import": "6.4.1",
    "babel-plugin-transform-define": "2.0.0",
    "bad-words": "3.0.3",
